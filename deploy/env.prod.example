# Telegram Bot Configuration
tg_bot_token=YOUR_TELEGRAM_BOT_TOKEN

# Application Environment
app_env=vps

# API Configuration
api_host=0.0.0.0
api_port=8000

# Redis Configuration
# Для Docker используйте: redis://redis:6379/0
# Для внешнего Redis: redis://your-redis-host:6379/0
redis_url=redis://redis:6379/0

# Fal.ai API Configuration
fal_api_key=YOUR_FAL_API_KEY
fal_queue_base_url=https://queue.fal.run

# Model Configuration
fal_standard_model=fal-ai/flux-pro/v1.1-ultra
fal_premium_model=fal-ai/nano-banana
fal_inpaint_model=fal-ai/flux-general/inpainting
fal_edit_model=fal-ai/chrono-edit
fal_upscale_model=fal-ai/recraft/upscale/crisp
fal_upscale_fallback_model=
fal_retoucher_model=fal-ai/retoucher
fal_face_enhance_model=fal-ai/image-editing/face-enhancement
fal_face_swap_model=fal-ai/face-swap

# Media Storage
media_dir=./media

# Logging
log_level=INFO

# Video Configuration (если используется)
video_poll_interval=5
video_max_wait_sec=900

